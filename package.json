{
	"name": "rsc-pkg",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "node --loader ts-node/esm scripts/build.ts",
		"dev": "node --loader ts-node/esm src/index.tsx",
		"fix:format": "prettier --write \"**/*.{json,js,ts,tsx}\"",
		"fix:lint": "eslint --fix --ext .json,.js,.ts,.tsx .",
		"fix": "yarn fix:lint && yarn fix:format",
		"check:format": "prettier --check \"**/*.{json,js,ts,tsx}\"",
		"check:lint": "eslint --ext .json,.js,.ts,.tsx .",
		"husky:pre-commit": "yarn check:lint && yarn check:format",
		"husky:pre-push": "yarn check:lint && yarn check:format",
		"release": "release-it"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "4.2.0",
		"ink-text-input": "5.0.1",
		"ink-select-input": "5.0.0",
		"meow": "12.0.1",
		"react": "18.2.0"
	},
	"devDependencies": {
		"@types/node": "20.2.5",
		"@types/react": "18.2.7",
		"@typescript-eslint/eslint-plugin": "5.59.8",
		"@typescript-eslint/parser": "5.59.8",
		"chalk": "5.2.0",
		"eslint": "8.41.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"husky": "8.0.3",
		"prettier": "2.8.8",
		"release-it": "15.10.3",
		"ts-node": "10.9.1",
		"typescript": "5.0.4"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}