{
	"name": "rsc-pkg",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"fix:format": "prettier --write \"**/*.{js,ts,tsx}\"",
		"fix:lint": "eslint --fix --ext .ts,.tsx .",
		"fix": "yarn fix:lint && yarn fix:format",
		"check:format": "prettier --check \"**/*.{js,ts,tsx}\"",
		"check:lint": "eslint --ext .ts,.tsx .",
		"husky:pre-commit": "yarn check:lint && yarn check:format",
		"husky:pre-push": "yarn check:lint && yarn check:format",
		"release": "release-it"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "4.1.0",
		"meow": "11.0.0",
		"react": "18.2.0"
	},
	"devDependencies": {
		"@types/react": "18.0.32",
		"@typescript-eslint/eslint-plugin": "5.59.2",
		"@typescript-eslint/parser": "5.59.2",
		"chalk": "5.2.0",
		"eslint": "8.39.0",
		"eslint-config-prettier": "8.7.0",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"husky": "8.0.3",
		"ink-testing-library": "3.0.0",
		"prettier": "2.8.7",
		"release-it": "15.10.3",
		"ts-node": "10.9.1",
		"typescript": "5.0.3"
	}
}