{
  "name": "rsc-pkg",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "build": "node --loader ts-node/esm scripts/build.ts",
    "build:local": "pnpm build && chmod +x dist/index.mjs && npm install -g .",
    "dev": "node --loader ts-node/esm src/index.tsx",
    "fix:format": "prettier --write \"**/*.{json,js,ts,tsx}\"",
    "fix:lint": "eslint --fix --ext .json,.js,.ts,.tsx .",
    "fix": "pnpm fix:lint && yarn fix:format",
    "check:format": "prettier --check \"**/*.{json,js,ts,tsx}\"",
    "check:lint": "eslint --ext .json,.js,.ts,.tsx .",
    "husky:pre-commit": "pnpm check:lint && pnpm check:format",
    "husky:pre-push": "pnpm check:lint && pnpm check:format",
    "release": "release-it"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ink": "4.2.0",
    "ink-select-input": "5.0.0",
    "ink-text-input": "5.0.1",
    "meow": "12.0.1",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "chalk": "5.2.0",
    "esbuild": "0.18.12",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "prettier": "2.8.8",
    "release-it": "15.11.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
