{
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/*.cjs",
    "dist/*.mjs",
    "dist/*.map",
    "dist/*.ts"
  ],
  "scripts": {
    "build":
      "pnpm clean && tsx scripts/build.ts && tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "esbuild": "0.19.2"
  }
}
